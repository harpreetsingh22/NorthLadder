{
  "name": "@northladder",
  "description": "northladder APIs",
  "version": "1.0.0",
  "license": "RDPL",
  "engines": {
    "npm": ">=10.2.3 <11",
    "node": ">=18.19.0 <21"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "NODE_OPTIONS=--openssl-legacy-provider node ./dist/bin/run.js --server",
    "dev": "NODE_OPTIONS=--openssl-legacy-provider nodemon --exec ts-node bin/run.ts --server",
    "migrate:dev": "knex --env development migrate:latest",
    "migrate:staging": "knex --env staging migrate:latest",
    "migrate:production": "knex --env production migrate:latest",
    "lint:check": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix --ext .ts",
    "prettier:check": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.495.0",
    "@aws-sdk/client-eventbridge": "^3.495.0",
    "@aws-sdk/client-secrets-manager": "^3.614.0",
    "@aws-sdk/client-sns": "^3.495.0",
    "@aws-sdk/lib-dynamodb": "^3.495.0",
    "@aws-sdk/util-dynamodb": "^3.495.0",
    "@datadog/datadog-api-client": "^1.21.0",
    "@hapi/boom": "^10.0.1",
    "@hapi/catbox-memory": "^6.0.1",
    "@hapi/catbox-redis": "^7.0.2",
    "@hapi/glue": "^9.0.1",
    "@hapi/hapi": "^21.3.2",
    "@hapi/hoek": "^11.0.4",
    "@hapi/joi-date": "^2.0.1",
    "@hapi/wreck": "^18.0.1",
    "@hapipal/haute-couture": "^4.3.1",
    "@hapipal/schmervice": "^3.0.0",
    "@hapipal/schwifty": "git+https://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:x-oauth-basic@github.com:rariohq/schwifty.git",
    "@hapipal/toys": "^4.0.0",
    "crypto": "^1.0.1",
    "dd-trace": "^5.0.0",
    "dotenv": "^16.3.2",
    "exiting": "^6.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.12.0",
    "knex": "^3.1.0",
    "luxon": "^3.4.4",
    "minimist": "^1.2.8",
    "mysql2": "^3.7.1",
    "nanoid": "3.3.5",
    "objection": "^3.1.3",
    "string-similarity": "^4.0.4",
    "unique-names-generator": "^4.7.1",
    "uuid": "^10.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@hapipal/types-haute-couture": "^0.0.1-alpha.1",
    "@types/hapi__glue": "^6.1.9",
    "@types/luxon": "^3.4.1",
    "@types/minimist": "^1.2.5",
    "@types/mysql": "^2.15.25",
    "@types/node": "^20.11.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.ts": [
      "npm run prettier:check",
      "npm run lint:check"
    ]
  }
}
